# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_access_key_id_here
AWS_SECRET_ACCESS_KEY=your_secret_access_key_here

# Amazon Cognito Configuration
COGNITO_USER_POOL_ID=your_user_pool_id_here
COGNITO_CLIENT_ID=your_client_id_here
COGNITO_CLIENT_SECRET=your_client_secret_here  # Optional: Only if you generated a client secret

# DynamoDB Configuration
DYNAMODB_TABLE_NAME=ops-care-dash
DYNAMODB_SIM_REQUEST_TABLE=ops-care-sim-requests
DYNAMODB_SIM_RESULT_TABLE=ops-care-sim-results

# Server Configuration
PORT=5000
JWT_SECRET=your_jwt_secret_here

# Optional: For development/testing
NODE_ENV=development
